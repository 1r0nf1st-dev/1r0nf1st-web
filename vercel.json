{
  "buildCommand": "pnpm build:server && pnpm build",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/catchall?x-path=:path*"
    }
  ],
  "functions": {
    "api/catchall.ts": {
      "includeFiles": "server/dist/**",
      "maxDuration": 10
    }
  }
}
